<Window x:Class="Aplicacion.Presentacion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aplicacion.Presentacion"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Button Style="{StaticResource Clientes_}" Name="btnGestionClientes" Content="" HorizontalAlignment="Left" Margin="266,193,0,0" VerticalAlignment="Top"  Click="btnGestionClientes_Click"/>
        <Button Style="{StaticResource Contratos_}" Name="btnGestionContratos" Content="" HorizontalAlignment="Left" Margin="431,193,0,0" VerticalAlignment="Top"   Click="btnGestionContratos_Click"/>
        <Image  Source="img/logo_BeLife.png" HorizontalAlignment="Left" Height="100" Margin="226,54,0,0" VerticalAlignment="Top" Width="289"/>


    </Grid>
</Window>
